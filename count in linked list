	         temp.next=newNode;
	     }
	     void count(){
	          int count=0;
	          Node temp=head;
	          while(temp!=null){
	               count++;
	               temp=temp.next;
	          }
	          System.out.println(count);
	     }
	     static void display(){
	          Node temp=head;
	          while(temp!=null){
	          System.out.println(temp.data+" ");
	          temp=temp.next;
	     }
	}
	public static void main(String[] args){
	     Scanner s=new Scanner(System.in);
	     Main list=new Main();
